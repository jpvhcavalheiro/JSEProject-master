package io.altar.jseproject.model;
import java.util.ArrayList;

public class Product extends Entity {
	private ArrayList <int> shelvesIdList;
	private int discount;
	private int iva;
	private int pvp;
	public Product(ArrayList<Shelf> shelvesIdList, int discount, int iva, int pvp) {
		super();
		this.shelvesIdList = shelvesIdList;
		this.discount = discount;
		this.iva = iva;
		this.pvp = pvp;
	}
	public ArrayList<Shelf> getShelvesIdList() {
		return shelvesIdList;
	}
	public void setShelvesList(ArrayList<int> shelvesIdList) {
		this.shelvesIdList = shelvesList;
	}
	public int getDiscount() {
		return discount;
	}
	public void setDiscount(int discount) {
		this.discount = discount;
	}
	public int getIva() {
		return iva;
	}
	public void setIva(int iva) {
		this.iva = iva;
	}
	public int getPvp() {
		return pvp;
	}
	public void setPvp(int pvp) {
		this.pvp = pvp;
	}
	@Override
	public String toString() {
		return "Product"+getId()+" [shelvesList=" + shelvesIdList + ", discount=" + discount + ", iva=" + iva + ", pvp=" + pvp + "]";
	}

	
}
